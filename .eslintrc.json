{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "array-bracket-spacing": [
      "warn",
      "always",
      {

      }
    ],
    "block-scoped-var": "error",
    "block-spacing": [
      "warn",
      "always"
    ],
    "brace-style": [
      "warn",
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": [
      "warn",
      {
        "properties": "always"
      }
    ],
    "comma-dangle": [
      "warn",
      "never"
    ],
    "comma-spacing": "warn",
    "comma-style": [
      "warn",
      "last"
    ],
    "computed-property-spacing": [
      "warn",
      "never"
    ],
    "consistent-this": [
      "error",
      "self"
    ],
    "default-case": "error",
    "dot-location": [
      "warn",
      "property"
    ],
    "eol-last": [
      "warn",
      "never"
    ],
    "eqeqeq": "error",
    "func-call-spacing": [
      "error",
      "never"
    ],
    "func-name-matching": [
      "warn",
      "always"
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "keyword-spacing": "warn",
    "max-depth": [
      "error",
      {
        "max": 3
      }
    ],
    "max-nested-callbacks": [
      "error",
      {
        "max": 3
      }
    ],
    "max-params": [
      "warn",
      {
        "max": 5
      }
    ],
    "max-statements-per-line": [
      "warn",
      {
        "max": 1
      }
    ],
    "new-cap": "error",
    "newline-after-var": [
      "warn",
      "always"
    ],
    "newline-before-return": "warn",
    "newline-per-chained-call": [
      "warn",
      {
        "ignoreChainWithDepth": 2
      }
    ],
    "no-alert": "error",
    "no-array-constructor": "error",
    "no-bitwise": "error",
    "no-caller": "error",
    "no-console": "warn",
    "no-debugger": "error",
    "no-empty-function": "error",
    "no-else-return": "error",
    "no-div-regex": "error",
    "no-eq-null": "error",
    "no-eval": "error",
    "no-extend-native": "error",
    "no-extra-bind": "warn",
    "no-floating-decimal": "error",
    "no-label-var": "warn",
    "no-lone-blocks": "warn",
    "no-lonely-if": "warn",
    "no-loop-func": "error",
    "no-magic-numbers": [
      "error",
      {
        "ignoreArrayIndexes": true,
        "enforceConst": true
      }
    ],
    "no-multi-assign": "error",
    "no-multi-spaces": [
      "warn",
      {
        "exceptions": {
          "Property": true
        }
      }
    ],
    "no-nested-ternary": "error",
    "no-new-func": "error",
    "no-new-object": "error",
    "no-redeclare": "error",
    "no-return-assign": "error",
    "no-shadow-restricted-names": "error",
    "no-throw-literal": "warn",
    "no-undef-init": "error",
    "no-undefined": "error",
    "no-unused-vars": "warn",
    "no-use-before-define": "error",
    "no-useless-call": "warn",
    "no-useless-concat": "error",
    "no-useless-return": "error",
    "no-unreachable": "error",
    "no-warning-comments": "warn",
    "no-whitespace-before-property": "warn",
    "no-with": "error",
    "object-curly-newline": [
      "warn",
      {
        "minProperties": 2
      }
    ],
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "object-property-newline": "warn",
    "one-var-declaration-per-line": "warn",
    "padded-blocks": [
      "warn",
      {
        "classes": "always"
      }
    ],
    "quotes": [
      "error",
      "single"
    ],
    "radix": "warn",
    "space-before-blocks": [
      "warn",
      "always"
    ],
    "space-in-parens": [
      "warn",
      "never"
    ],
    "space-infix-ops": "warn",
    "space-unary-ops": [
      "warn",
      {
        "words": true,
        "nowords": false
      }
    ],
    "vars-on-top": "warn",
    "semi": [
      "error",
      "never"
    ],
    "yoda": "error"
  }
}
